generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model School {
  id             String   @id @default(uuid())
  name           String
  city           String
  contact_number String
  email          String
  Teachers       String[]
  Students       String[]
  Courses        String[]
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
